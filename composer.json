{
	"name": "queryr/replicator",
	"require": {
		"php": ">=5.5.0",
		"wikibase/data-model": "0.7.*",
		"wikibase/database": "dev-master",
		"wikibase/query-engine": "dev-master",

		"wikibase/internal-serialization": "~1.0@dev",
		"data-values/common": "*",
		"data-values/geo": "*",
		"data-values/number": "*",
		"data-values/time": "*",

		"symfony/console": "~2.3.11"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0.0"
	},
	"minimum-stability": "dev",
	"autoload": {
		"psr-4": {
			"Wikibase\\Dump\\": "src/Dump/",
			"QueryR\\Replicator\\": "src/Replicator/",
			"Tests\\Fixtures\\": "tests/fixtures/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.1.x-dev"
		}
	},
	"bin": [
		"app/replicator.php"
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "/home/j/workspace/WikibaseQueryEngine"
		}
	]
}
